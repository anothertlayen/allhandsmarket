{
  "version": 2,
  "buildCommand": "node vercel-build-override.js && npm run vercel-build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "git": {
    "deploymentEnabled": {
      "openhands-workspace-dji921zx": true
    }
  },
  "ignoreCommand": "exit1",
  "env": {
    "MONGODB_URI": "your-mongodb-uri",
    "OPENAI_API_KEY": "your-openai-api-key",
    "NEXTAUTH_SECRET": "your-nextauth-secret-generated-with-crypto-randomBytes",
    "NEXTAUTH_URL": "your-deployment-url.vercel.app",
    "NEXT_PUBLIC_SKIP_TYPE_CHECK": "true",
    "NEXT_PUBLIC_SKIP_LINT": "true"
  }
}